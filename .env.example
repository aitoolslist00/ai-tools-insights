# Performance optimizations
NEXT_TELEMETRY_DISABLED=1

# Bundle analyzer
ANALYZE=false

# Experimental features
NEXT_EXPERIMENTAL_OPTIMIZE_PACKAGE_IMPORTS=true
NEXT_EXPERIMENTAL_OPTIMIZE_CSS=true

# Performance monitoring
NEXT_PERFORMANCE_MONITORING=true

# Blog API Configuration
BLOG_API_URL=your_blog_api_url_here
BLOG_API_KEY=your_blog_api_key_here

# Blog Dashboard Authentication
JWT_SECRET=your_jwt_secret_here
ADMIN_USERNAME=your_admin_username_here
ADMIN_PASSWORD_HASH=your_bcrypt_password_hash_here

# Analytics (optional)
GOOGLE_ANALYTICS_ID=your_ga_id_here
VERCEL_ANALYTICS_ID=your_vercel_analytics_id_here

# News API for real-time content (optional but recommended)
NEWS_API_KEY=your_newsapi_key_here

# Alternative news sources (optional)
GOOGLE_NEWS_API_KEY=your_google_news_api_key_here
BING_NEWS_API_KEY=your_bing_news_api_key_here

# Unsplash API for image generation
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Gemini AI for enhanced image search and content analysis
GEMINI_API_KEY=your_gemini_api_key_here